{
  "name": "@kunukn/react-collapse",
  "author": "Kunuk Nykjaer",
  "license": "MIT",
  "private": false,
  "version": "3.0.0",
  "type": "module",
  "main": "./dist/react-collapse.umd.cjs",
  "module": "./dist/react-collapse.js",
  "types": "./dist/react-collapse.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-collapse.js",
      "require": "./dist/react-collapse.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "description": "Collapse library based on CSS transition for React",
  "keywords": [
    "react",
    "collapsible",
    "accordion",
    "CSS-transition",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/kunukn/react-collapse/issues"
  },
  "homepage": "https://github.com/kunukn/react-collapse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/react-collapse.git"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18.20",
    "npm": ">=9"
  },
  "scripts": {
    "start": "vite --host --open --port 3009",
    "dev": "vite --host --open --port 3009",
    "build": "npm run cleandist && tsc --p ./tsconfig-build.json && vite build && npm run copyfiles",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyfiles": "node node-tools/copy-files.js",
    "cleandist": "node node-tools/clean-dist.js",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@types/node": "^18.19.29",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "copy-file": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.74.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vite-plugin-remove-console": "^2.2.0"
  }
}
